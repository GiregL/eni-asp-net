@model BO.Model.ArtMartial

@{
    ViewData["Title"] = "Détails d'un art martial";
}

<h1>@ViewBag.Title</h1>

<div>
    <h4>Art martial : @Model.Nom</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nom)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nom)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Éditer</a> |
    <a asp-action="Index">Retour à la liste</a>
</div>
